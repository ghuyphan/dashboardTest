{
  "name": "hoan-my-portal",
  "version": "0.7.18",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "version:patch": "node scripts/bump-version.js patch",
    "version:minor": "node scripts/bump-version.js minor",
    "version:major": "node scripts/bump-version.js major",
    "build:prod:patch": "npm run version:patch && ng build --configuration production",
    "build:prod:minor": "npm run version:minor && ng build --configuration production",
    "build:prod:major": "npm run version:major && ng build --configuration production",
    "lint": "ng lint",
    "prepare": "husky",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,js,html,json,scss,css,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.17",
    "@angular/cdk": "^19.2.19",
    "@angular/cdk-experimental": "^19.2.19",
    "@angular/common": "^19.2.16",
    "@angular/compiler": "^19.2.16",
    "@angular/core": "^19.2.16",
    "@angular/forms": "^19.2.16",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.16",
    "@angular/platform-browser-dynamic": "^19.2.16",
    "@angular/router": "^19.2.16",
    "@e965/xlsx": "^0.20.3",
    "@pdfme/common": "^5.5.7",
    "@pdfme/generator": "^5.5.7",
    "@types/dompurify": "^3.0.5",
    "angularx-qrcode": "^19.0.0",
    "d3": "^7.9.0",
    "dompurify": "^3.3.0",
    "echarts": "^6.0.0",
    "file-saver": "^2.0.5",
    "marked": "^17.0.1",
    "pdf-lib": "^1.17.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.18",
    "@angular/cli": "^19.2.18",
    "@angular/compiler-cli": "^19.2.0",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.46.4"
  }
}
